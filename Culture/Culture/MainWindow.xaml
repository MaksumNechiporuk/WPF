        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Culture"      
        xmlns:p="clr-namespace:Culture.Properties"
        mc:Ignorable="d"  
        Title ="{x:Static p:Resources.MainWindow_Title}" Height="450" Width="800">
   
    
    <Grid>
        <Button x:Name="btnSetUk" Content="Українська мова" HorizontalAlignment="Left" Margin="117,175,0,0" VerticalAlignment="Top" Width="150" Height="48" FontSize="16" Click="btnSetUk_Click"/>
        <Button x:Name="btnSetRu" Content="Русский язык" HorizontalAlignment="Left" Margin="498,175,0,0" VerticalAlignment="Top" Width="150" Height="48" FontSize="16" Click="btnSetRu_Click"/>

    </Grid>
</Window>
